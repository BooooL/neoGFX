{
    resource: {
        prefix: test
        files: [
            resources/neoGFX.png
            resources/channel.png
            resources/channel_32.png
            resources/channel_128.png
            resources/channel_256.png
            resources/orca.png
            resources/explosion.png
            resources/icon.png
        ]
    }

    ui: {
        app: {
            id: app
            name: "neoGFX Test App"
            default_window_icon: ":/test/resources/icon.png"
            action: {
                id: actionGoldenrodStyle
                text: "Goldenrod Style"
                shortcut: "Ctrl+Alt+Shift+G"
            }
            action: {
                id: actionContacts
                text: "&Contacts..."
                image_uri: ":/closed/resources/caw_toolbar.naa#contacts.png"
                shortcut: "Alt+C"
            }
            action: {
                id: actionAddFavourite
                text: "Add Favourite..."
                image_uri: ":/closed/resources/caw_toolbar.naa#add_favourite.png"
            }
            action: {
                id: actionOrganizeFavourites
                text: "Organize Favourites..."
                image_uri: ":/closed/resources/caw_toolbar.naa#organize_favourites.png"
                shortcut: "Alt+C"
            }
            action: {
                id: actionMute
                text: "Mute/&Unmute Sound"
                image_uri: ":/closed/resources/caw_toolbar.naa#mute.png"
                checkable: true
                checked_image_uri: ":/closed/resources/caw_toolbar.naa#unmute.png"
            }
            action: {
                id: actionPasteAndGo
                text: "Paste and Go"
                image_uri: ":/closed/resources/caw_toolbar.naa#paste_and_go.png"
                shortcut: "Ctrl+Shift+V"
            }
            window: {
                id: window
                default_width: 800
                default_height: 800
                status_bar: {
                    id: statusBar
                }
                menu_bar: {
                    id: menu
                    menu: {
                        title: "&File"
                        action: app.actionFileNew
                        action: app.actionFileOpen
                        action: separator
                        action: app.actionFileClose
                        action: separator
                        action: app.actionFileSave
                        action: separator
                        action: app.actionFileExit
                    }
                    menu: {
                        title: "&Edit"
                        action: app.actionUndo
                        action: app.actionRedo
                        action: separator
                        action: app.actionCut
                        action: app.actionCopy
                        action: app.actionPaste
                        action: pasteAndGoAction
                        action: app.actionDelete
                        action: separator
                        action: app.actionSelectAll
                    }
                    menu: {
                        title: "&View"
                    }
                    menu: {
                        title: "F&avourites"
                        action: actionAddFavourite
                        action: actionOrganizeFavourites
                        action: separator
                        action: { text: "Alice" image_uri: ":/closed/resources/caw_toolbar.naa#favourite.png" }
                        action: { text: "Bob" image_uri: ":/closed/resources/caw_toolbar.naa#favourite.png" }
                        action: { text: "Carlos" image_uri: ":/closed/resources/caw_toolbar.naa#favourite.png" }
                        action: { text: "Dave" image_uri: ":/closed/resources/caw_toolbar.naa#favourite.png" }
                        menu: {
                            title: "Silent Running Drones"
                            image_uri: ":/closed/resources/caw_toolbar.naa#folder.png"
                            action: { text: "Dewey" image_uri: ":/closed/resources/caw_toolbar.naa#favourite.png" }
                            action: { text: "Huey" image_uri: ":/closed/resources/caw_toolbar.naa#favourite.png" }
                            action: { text: "Louie" image_uri: ":/closed/resources/caw_toolbar.naa#favourite.png" }
                        }
                        menu: {
                            title: "DC (TM) Characters"
                            image_uri: ":/closed/resources/caw_toolbar.naa#folder.png"
                        }
                        menu: {
                            title: "Marvel (TM) Characters"
                            image_uri: ":/closed/resources/caw_toolbar.naa#folder.png"
                        }
                        action: separator
                    }
                    action: actionContacts
                    action: actionMute
                    action: { text: "&Xyzzy..." image_uri: ":/neogfx/resources/icons.naa#eyedropper.png" }
                    menu: {
                        id: testMenu
                        title: "&Test"
                        action: {
                            id: actionEmacsStyleSequence1
                            text: "Emacs style sequence #1"
                            shortcut: "Ctrl+K, Ctrl+B"
                        }
                        action: {
                            id: actionEmacsStyleSequence2
                            text: "Emacs style sequence #2"
                            shortcut: "Ctrl+K, Ctrl+K"
                        }
                        action: actionContacts
                        action: actionMute
                        action: actionMute
                        action: actionMute
                        action: actionMute
                        action: {
                            id: actionColourDialog
                            text: "Colour Dialog..."
                        }
                    }
                    menu: {
                        id: windowMenu
                        title: "&Window"
                        action: {
                            id: actionNextTab
                            text: "Next Tab"
                            shortcut: "Ctrl+Tab"
                        }
                        action: {
                            id: actionPreviousTab
                            text: "Previous Tab"
                            shortcut: "Shift+Ctrl+Tab"
                        }
                    }
                    menu: {
                        id: helpMenu
                        title: "&Help"
                    }
                }
                toolbar: {
                    id: toolbar
                    action: app.actionFileNew
                    action: app.actionFileOpen
                    action: app.actionFileSave
                    action: separator
                    action: actionContacts
                    action: actionAddFavourite
                    action: actionOrganizeFavourites
                    action: { text: "Keywords..." image_uri: ":/closed/resources/caw_toolbar.naa#keyword.png" }
                    action: { text: "Settings..." image_uri: ":/closed/resources/caw_toolbar.naa#settings.png" }
                    action: { id: actionManagePlugins text: "Manage Plugins..." image_uri: ":/closed/resources/caw_toolbar.naa#manage_plugins.png" }
                    action: actionMute
                    action: separator
                    action: app.actionCut
                    action: app.actionCopy
                    action: app.actionPaste
                    action: actionPasteAndGo
                    action: separator
                    action: { id: actionCheckForUpdates text: "Check for Updates..." image_uri: ":/closed/resources/caw_toolbar.naa#setup.png" }
                }
            }
        }
    }
}
